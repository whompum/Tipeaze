<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  name="colorPrimary" format="color|reference"/>  given by default in AOS, so exclude here-->
    <attr name="colorMain" format="color|reference"/>
    <attr name="colorSecondary" format="color|reference"/>
    <attr name="colorTertiary" format="color|reference"/>
    <attr name="colorBackground" format="color|reference"/>


    <!--Below are the attributes of all items within the layouts -->
    <attr name="tipViewCardHighlight" format="color|reference"/>
    <attr name="addFabBackground" format="color|reference"/>
    <attr name="personalTabsCurrentTextHighlight" format="color|reference"/>
    <attr name="personalTabsSecondaryTextHighlight" format="color|reference"/>
    <attr name="addFabiconHighlight" format="color|reference"/>
    <attr name="presetDropdownHighlight" format="color|reference"/>
    <attr name="billContentHighlight" format="color|reference"/>
    <attr name="billTotalHighlight" format="color|reference"/>

    <declare-styleable name="TipView">
        <attr name="thumbHighlight" format="color|reference"/>
        <attr name="thumbTouchHighlight" format="color|reference"/>
        <attr name="thumbTouchTextHighlight" format="color|reference"/>
        <attr name="thumbSize" format="dimension|reference"/>
        <attr name="thumbTextSize" format="dimension|reference"/>
        <attr name="thumbTextHighlight" format="color|reference"/>
        <attr name="tipThumbPadding" format="dimension|reference"/>

        <attr name="totalTrackSize" format="dimension|reference"/>
        <attr name="trackHighlight" format="color|reference"/>
        <attr name="trackStrokeSize" format="dimension|reference"/>
        <attr name="trackStrokeHighlight" format="color|reference"/>
        <attr name="trackTextHighlight" format="color|reference"/>
        <attr name="trackTextSize" format="dimension|reference"/>

        <attr name="tickmarkHighlight" format="color|reference"/>
        <attr name="tickmarkWidth" format="dimension|reference"/>
    </declare-styleable>


    <declare-styleable name="RobotoBasedTextview">
        <attr name="font" format="enum">
            <enum name="BOLD" value="0"/>
            <enum name="LIGHT" value="1"/>
            <enum name="MEDIUM" value="2"/>
            <enum name="REGULAR" value="3"/>
            <enum name="THIN" value="4"/>
        </attr>
    </declare-styleable>

</resources>